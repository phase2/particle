{
  "name": "PatternLabStarter",
  "version": "2.4.0",
  "description": "Pattern Lab Starter",
  "main": "Gruntfile.js",
  "author": {
    "name": "Evan Lovely",
    "url": "https://github.com/EvanLovely"
  },
  "contributors": [
    {
      "name": "Chris Bloom",
      "url": "https:/github.com/illepic"
    },
    {
      "name": "Micah Godbolt",
      "url": "https:/github.com/micahgodbolt"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:phase2/pattern-lab-starter.git"
  },
  "keywords": [
    "pattern-lab",
    "prototyping"
  ],
  "scripts": {
    "prestart": "npm install",
    "start": "grunt",
    "precompile": "npm install",
    "compile": "grunt compile",
    "test": "npm run compile && grunt validate --noTestForce",
    "predeploy": "grunt compile",
    "postinstall": "bower install && bundle install && find node_modules/ -name \"*.info\" -type f -delete",
    "gh-pages": "git checkout gh-pages && git merge master --no-ff && npm run deps && npm run compile && git add --all :/ && git commit -m 'GitHub Pages Site Update' && git push && git checkout - && echo '' && echo 'Content from master merged into gh-pages, compiled, commited, and pushed. Should be live on http://phase2.github.io/pattern-lab-starter soon'"
  },
  "paths": {
    "patternlab": "",
    "drupalTheme": ""
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "bourbon": "^4.2.3",
    "bower": "^1.3.12",
    "breakpoint-sass": "^2.6.1",
    "chalk": "^0.5.1",
    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.1",
    "grunt-browser-sync": "^2.1.3",
    "grunt-bump": "^0.3.1",
    "grunt-cli": "^0.1.13",
    "grunt-concurrent": "^1.0.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-connect": "^0.9.0",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-injector": "^0.6.0",
    "grunt-jsonlint": "^1.0.4",
    "grunt-newer": "^1.1.0",
    "grunt-notify": "^0.4.1",
    "grunt-open": "^0.2.3",
    "grunt-pattern-lab-component-builder": "0.0.12",
    "grunt-phantomcss": "git://github.com/micahgodbolt/grunt-phantomcss.git",
    "grunt-postcss": "^0.5.5",
    "grunt-sass": "^1.0.0",
    "grunt-sass-globbing": "^1.3.0",
    "grunt-scss-lint": "git://github.com/EvanLovely/grunt-scss-lint.git#master",
    "grunt-shell": "^1.1.1",
    "grunt-webfont": "^0.5.4",
    "grunt-wiredep": "^2.0.0",
    "inquirer": "^0.8.5",
    "load-grunt-tasks": "^3.1.0",
    "lodash": "^3.6.0",
    "normalize.scss": "^0.1.0",
    "phantomjs": "^1.9.16",
    "singularitygs": "^1.6.2",
    "time-grunt": "^1.0.0"
  },
  "plbuild": {
    "openBrowserAtStart": true,
    "scssDir": "scss",
    "cssDir": "css",
    "jsDir": "js",
    "plDir": "pattern-lab",
    "iconsDir": "images/icons",
    "serverDir": "./",
    "serverPath": "pattern-lab/public/",
    "updateAtStart": true,
    "buildModernizrAtStart": true,
    "regressionTestRoot": "pattern-lab/source/_patterns",
    "gruntModules": [
      {
        "module": "pattern-lab",
        "enabled": true
      },
      {
        "module": "sass",
        "enabled": true
      },
      {
        "module": "javascript",
        "enabled": true
      },
      {
        "module": "jshint",
        "enabled": false
      },
      {
        "module": "drupal7",
        "enabled": false
      },
      {
        "module": "icons",
        "enabled": false
      },
      {
        "module": "regression-qa",
        "enabled": true
      }
    ]
  }
}
